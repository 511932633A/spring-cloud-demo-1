<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zzwy.dao.UserBonusrMapper">

    <!--更新终端状态-->
    <update id="addBouns" parameterType="java.util.Map">
        UPDATE user_bonus SET bonus = bonus + #{bonus} WHERE
        user_id = #{userId}
    </update>


</mapper>